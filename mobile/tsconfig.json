{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@assets/*": [
        "./assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@sheets/*": [
        "./src/sheets/*"
      ],
      "@util/*": [
        "./src/util/*"
      ],
    }
  }
}